    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "rimraf": "^5.0.1",
    "rollup": "^3.22.0",
